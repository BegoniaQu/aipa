<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="basicPoolConfig" class="redis.clients.jedis.JedisPoolConfig">
		<property name="maxIdle" value="20" />
		<property name="minIdle" value="10" />
		<property name="maxTotal" value="50" />
		<property name="maxWaitMillis" value="15000" />
	</bean>

	<bean id="basicJedisPool" class="redis.clients.jedis.JedisPool"
		destroy-method="destroy">
		<constructor-arg name="poolConfig" ref="basicPoolConfig" />
		<constructor-arg name="host" type="java.lang.String"
			value="${basic.redis.cache.host}" />
		<constructor-arg name="port" value="${basic.redis.cache.port}" />
		<constructor-arg name="timeout" value="${redis.timeout}" />
	</bean>
	
	<bean id="basicStringCacheRao" class="com.anlaiye.data.common.proxy.rao.impl.StringCacheRaoImpl">
		<property name="jedisPool" ref="basicJedisPool" />
		<property name="retryTimes" value="3" />
	</bean>
	
	<bean id="basicSetCacheRao" class="com.anlaiye.data.common.proxy.rao.impl.SetCacheRaoImpl">
		<property name="jedisPool" ref="basicJedisPool" />
		<property name="retryTimes" value="3" />
	</bean>
 
    <bean id="basicHashCacheRao" class="com.anlaiye.data.common.proxy.rao.impl.HashCacheRaoImpl">
        <property name="jedisPool" ref="basicJedisPool" />
        <property name="retryTimes" value="3" />
    </bean>
		
	<bean id="tupleLongDoubleConvert"
		class="com.anlaiye.data.common.rao.convert.TupleLongDoubleConvert" />
		
	<bean id="tupleStringDoubleConvert"
		class="com.anlaiye.data.common.rao.convert.TupleStringDoubleConvert" />

	<bean id="basicTupleLongIndexCacheRao" class="com.anlaiye.data.common.proxy.rao.impl.IndexCacheRaoImpl">
		<property name="tupleObjectDoubleConvert" ref="tupleLongDoubleConvert" />
		<property name="jedisPool" ref="basicJedisPool" />
		<property name="retryTimes" value="3" />
	</bean>
	
	<bean id="basicTupleStringIndexCacheRao" class="com.anlaiye.data.common.proxy.rao.impl.IndexCacheRaoImpl">
		<property name="tupleObjectDoubleConvert" ref="tupleStringDoubleConvert" />
		<property name="jedisPool" ref="basicJedisPool" />
		<property name="retryTimes" value="3" />
	</bean>
	
	<!-- app -->
	<bean id="announcementInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.AnnouncementInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:announce" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="announcementIndexRao" class="com.anlaiye.breakfast.basic.common.rao.impl.AnnouncementIndexRaoImpl">
		<property name="indexCacheRao" ref="basicTupleLongIndexCacheRao" />
		<property name="keyPrefix" value="b:b:acIndex" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="buildingManagerInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.BuildingManagerInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:buildingM" />
		<property name="seconds" value="${basic.second}"/>
	</bean>
	
	<bean id="schoolInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.SchoolInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:school" />
		<property name="seconds" value="${basic.second}"/>
	</bean>
	
	<bean id="buildingManagerBindingInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.BuildingManagerBindingInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:ssBindInfo" />
		<property name="seconds" value="${basic.second}"/>
	</bean>
	
	<bean id="supplierInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.SupplierInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:supplierInfo" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="supplierUserInfoRao" class="com.anlaiye.breakfast.basic.common.rao.impl.SupplierUserInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:supplierUserInfo" />
		<property name="seconds" value="${basic.second}"/>
	</bean>
	
	<bean id="supplierUserIndexRao" class="com.anlaiye.breakfast.basic.common.rao.impl.SupplierUserIndexRaoImpl">
		<property name="indexCacheRao" ref="basicTupleStringIndexCacheRao" />
		<property name="keyPrefix" value="b:b:supplierUserIndex" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="buildingManagerBindingIndexRao" class="com.anlaiye.breakfast.basic.common.rao.impl.BuildingManagerBindingIndexRaoImpl">
		<property name="indexCacheRao" ref="basicTupleLongIndexCacheRao" />
		<property name="keyPrefix" value="b:b:bmbanding" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="supplierUserInfoRaoImpl" class="com.anlaiye.breakfast.basic.common.rao.impl.SupplierUserInfoRaoImpl">
		<property name="hashCacheRao" ref="basicHashCacheRao" />
		<property name="keyPrefix" value="b:b:workingday" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="supplierUserIndexRaoImpl" class="com.anlaiye.breakfast.basic.common.rao.impl.SupplierUserIndexRaoImpl">
		<property name="indexCacheRao" ref="basicTupleLongIndexCacheRao" />
		<property name="keyPrefix" value="b:g:workingday" />
		<property name="seconds" value="${basic.second}"/>
	</bean>

	<bean id="basicDistributeLockFactory" class="com.anlaiye.data.common.lock.impl.RedisDistributeLockFactory">
		<property name="jedisPool" ref="basicJedisPool"/>
		<property name="retryTimes" value="3"/>
		<property name="keyPrefix" value="basic"/>
	</bean>
</beans>